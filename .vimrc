set tabstop=4
set shiftwidth=4
set ruler
set smarttab
set et
set showmatch 
set hlsearch
set incsearch
set ignorecase
set ffs=unix,dos,mac
set fencs=utf-8,cp1251,koi8-r,ucs-2,cp866
syntax on
set nocompatible
set backspace=2
set encoding=utf-8
set fileencoding=utf8
:autocmd FileType make set noexpandtab
map <F2> :NERDTree <CR>
